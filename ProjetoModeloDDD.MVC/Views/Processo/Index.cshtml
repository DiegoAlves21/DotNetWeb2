@model IEnumerable<ProjetoModeloDDD.MVC.ViewModels.ProcessoViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.numeroProcesso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.relatoFiscalizacao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dataRelato)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fiscalResponsavel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.numeroProcesso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.relatoFiscalizacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dataRelato)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fiscalResponsavel)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProcessoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProcessoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProcessoId })
        </td>
    </tr>
}

</table>
